# .env.example

# Path to your Docker Compose file
COMPOSE_FILE=docker-compose.yaml

# Output directory for generated configs
OUTPUT_DIR=proxy/config/dynamic

# Domain suffix for services
DOMAIN_SUFFIX=.ddev

# External host IP address
EXTERNAL_HOST=

# Enable Docker Swarm mode (true/false)
SWARM_MODE=false

# Watch for file changes and regenerate (true/false)
WATCH=false

# Default backend host for HTTP services
DEFAULT_BACKEND_HOST=